<script type="text/javascript">

	var modal = document.querySelector(".modal-box-aboutus");
    var trigger = document.querySelector(".trigger-modal-aboutus");
	var triggermobile = document.querySelector(".trigger-modal-aboutus-mobile");
    var closeButton = document.querySelector(".close-button-modal-aboutus");

    function toggleModal() {
        modal.classList.toggle("show-modal-box-aboutus");
    }



    trigger.addEventListener("click", toggleModal);
	triggermobile.addEventListener("click", toggleModal);
    closeButton.addEventListener("click", toggleModal);
    window.addEventListener("click", windowOnClick);





	var modalinfo = document.querySelector(".modal-box-info");
    var triggerinfo = document.querySelector(".trigger-modal-info");
    var closeButtoninfo = document.querySelector(".close-button-modal-info");

    function toggleModalInfo() {
        modalinfo.classList.toggle("show-modal-box-info");
    }

    triggerinfo.addEventListener("click", toggleModalInfo);
    closeButtoninfo.addEventListener("click", toggleModalInfo);


    function windowOnClick(event) {
    if (event.target === modal) {
        toggleModal();
    }
	if (event.target === modalinfo) {
        toggleModalInfo();
    }
    }

</script>





